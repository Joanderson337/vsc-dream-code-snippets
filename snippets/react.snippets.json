{
  "react.fc": {
    "key": "react.fc",
    "prefix": "react.fc",
    "body": [
      "import classnames from 'classnames'",
      "import { FC } from 'react'",
      "import { useCssHandles } from 'vtex.css-handles'",
      "",
      "// import './styles.css'",
      "",
      "const CSS_HANDLES = [",
      "  'my__react__fc__component',",
      "  'my__react__fc__component__container',",
      "]",
      "",
      "interface ${1:${TM_FILENAME_BASE}}Props {",
      "// my component props",
      "}",
      "",
      "const ${1:${TM_FILENAME_BASE}}: FC<${1:${TM_FILENAME_BASE}}Props> = () => {",
      "  const handles = useCssHandles<string[]>(CSS_HANDLES)",
      "",
      "  return (",
      "     <div className={handles['my__store__fc__component']}>",
      "        <span>My React FC Component.</span>",
      "     </div>",
      "  );",
      "}",
      "",
      "export default ${1:${TM_FILENAME_BASE}}"
    ],
    "description": "Creates a React Functional Component with defaults Vtex"
  },
  "react.tscontext": {
    "key": "react.tscontext",
    "prefix": "react.tscontext",
    "body": [
      "import { createContext, useContext, useState } from 'react'",
      "",
      "interface MyReactContextProps {}",
      "",
      "type MyReactContextData = {",
      "  firstState: string",
      "}",
      "",
      "export const MyReactContext = createContext({} as MyReactContextData)",
      "",
      "export const useMyReactContext = () => {",
      "  return useContext(MyReactContext)",
      "}",
      "",
      "const MyReactContextProvider: MyReactContextProps = ({children}) => {",
      "  // states",
      "  const [firstState] = useState('');",
      "",
      "  // functions",
      "",
      "  // hooks",
      "",
      "  return (",
      "      <MyReactContext.Provider",
      "        value={{firstState}}",
      "      >",
      "        <h1>My React Context</h1>",
      "        {children}",
      "      </MyReactContext.Provider>",
      "  )",
      "}",
      "",
      "export default MyReactContextProvider"
    ],
    "description": "Creates a React Context with Typescript"
  }
}
