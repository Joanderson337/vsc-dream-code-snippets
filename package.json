{
  "name": "dream-code-snippets",
  "displayName": "Dream Code Snippets",
  "description": "Code snippet, to increase productivity",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./src/snippets/vtex.snippets.json"
      },
      {
        "language": "typescript",
        "path": "./src/snippets/vtex.snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./src/snippets/vtex.snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./src/snippets/vtex.snippets.json"
      },
      {
        "language": "javascript",
        "path": "./src/snippets/react.snippets.json"
      },
      {
        "language": "typescript",
        "path": "./src/snippets/react.snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./src/snippets/react.snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./src/snippets/react.snippets.json"
      },
      {
        "language": "css",
        "path": "./src/snippets/css.snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.66.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5",
    "@vscode/test-electron": "^2.1.3"
  },
  "publisher": "Leonardo-Joanderson",
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#030518",
    "theme": "dark"
  },
  "keywords": [
    "react",
    "typescript",
    "dream",
    "css",
    "json"
  ],
  "repository": {
    "url": "https://github.com/Joanderson337/vsc-dream-code-snippets-"
  },
  "homepage": "https://github.com/Joanderson337/vsc-dream-code-snippets-",
  "bugs": {
    "url": "https://github.com/Joanderson337/vsc-dream-code-snippets-/issues"
  }
}